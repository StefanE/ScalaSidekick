<?xml version="1.0"?>

<!DOCTYPE ACTIONS SYSTEM "actions.dtd">

<ACTIONS>
    <ACTION NAME="Format">
        <CODE>
            org.scala.sidekick.ScalaSidekickPlugin.format(view);
        </CODE>
    </ACTION>
    <ACTION NAME="Rename">
        <CODE>
        org.scala.sidekick.ScalaSidekickPlugin.rename(textArea,view);
        </CODE>
    </ACTION>
    <ACTION NAME="OrganizeImports">
        <CODE>
        org.scala.sidekick.ScalaSidekickPlugin.organizeImports(textArea, view);
        </CODE>
    </ACTION>
    <ACTION NAME="Navigate">
        <CODE>
        org.scala.sidekick.ScalaSidekickPlugin.navigate(view);
        </CODE>
    </ACTION>
</ACTIONS>